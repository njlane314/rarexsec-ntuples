@PACKAGE_INIT@

include(CMakeFindDependencyMacro)
find_dependency(ROOT REQUIRED COMPONENTS @RAREXSEC_ROOT_COMPONENTS@)

if(@RAREXSEC_USE_SYSTEM_JSON@)
    find_dependency(nlohmann_json @RAREXSEC_JSON_VERSION@ CONFIG)
endif()

set_and_check(rarexsec_INCLUDE_DIR "@PACKAGE_CMAKE_INSTALL_INCLUDEDIR@")
set(rarexsec_INCLUDE_DIRS "${rarexsec_INCLUDE_DIR}")

include("${CMAKE_CURRENT_LIST_DIR}/rarexsecTargets.cmake")

set(rarexsec_COMPONENTS processing)
set(rarexsec_LIBRARIES rarexsec::processing)
set(rarexsec_processing_FOUND TRUE)
set(rarexsec_processing_LIBRARIES rarexsec::processing)
set(rarexsec_processing_INCLUDE_DIRS "${rarexsec_INCLUDE_DIR}")
check_required_components(rarexsec)
